[user]
	name = Luis Felipe
	email = godoiluisfelipe@gmail.com
[diff]
	tool = meld
[merge]
	tool = meld
[difftool]
	prompt = false
[core]
	pager = diff-so-fancy | less
	untrackedCache = true
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
	func = 146 bold
[submodule]
	recurse = true
	fetchJobs = 0
[rebase]
	autoStash = true
[alias]
  patch = apply --ignore-space-change --ignore-whitespace
[init]
  defaultBranch = master
[interactive]
	diffFilter = diff-so-fancy --patch
